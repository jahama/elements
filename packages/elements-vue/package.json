{
  "name": "elements-vue",
  "version": "3.0.1",
  "description": "This is the vue3 package of the integration layer for  @inovex.de/elements.",
  "scripts": {
    "build": "npm run clean && npm run compile && npm run bundle && npm run build.vetur && npm run build.web-types",
    "bundle": "rollup --config rollup.config.js",
    "clean": "shx rm -rf dist && shx rm -rf dist-transpiled",
    "compile": "npm run tsc",
    "tsc": "tsc -p .",
    "build.web-types": "node ./scripts/build-web-types.js",
    "build.vetur": "node ./scripts/build-vetur.js"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.esm.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist/",
    "css/"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/inovex/elements.git"
  },
  "author": "Benjamin Pagelsdorf <bpagelsdorf@inovex.de>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/inovex/elements/issues"
  },
  "homepage": "https://github.com/inovex/elements#readme",
  "devDependencies": {
    "@types/node": "^14.10.3",
    "change-case": "^4.1.2",
    "fs-extra": "^9.0.0",
    "rollup": "^2.32.1",
    "vue": "^3.1.0",
    "shx": "^0.3.2",
    "typescript": "^4.2.2"
  },
  "dependencies": {
    "@inovex.de/elements": "^3.0.1"
  },
  "vetur": {
    "tags": "dist/vetur/tags.json",
    "attributes": "dist/vetur/attributes.json"
  },
  "web-types": "dist/web-types.json"
}
